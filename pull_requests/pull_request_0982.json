            "pullrequest": {
                "title": "Added halo finding method kwargs to HaloCatalog",
                "type": "pullrequest",
                "links": {
                    "self": {
                        "href": "https://api.bitbucket.org/2.0/repositories/yt_analysis/yt/pullrequests/982"
