            "pullrequest": {
                "title": "Stop referring to the requirements file in the installation docs",
                "type": "pullrequest",
                "links": {
                    "self": {
                        "href": "https://api.bitbucket.org/2.0/repositories/yt_analysis/yt/pullrequests/1839"
