pan class=\"p\">((</span><span class=\"mi\">2048</span><span class=\"p\">,</span><span class=\"mi\">2048</span><span class=\"p\">))</span>\n    <span class=\"c1\"># Force rebuild</span>\n    <span class=\"n\">sl</span><span class=\"p\">[</span><span class=\"s1\">&#39;density&#39;</span><span class=\"p\">]</span>\n    <span class=\"k\">try</span><span class=\"p\">:</span>\n        <span class=\"k\">assert</span> <span class=\"n\">np</span><span class=\"o\">.</span><span class=\"n\">all</span><span class=\"p\">(</span><span class=\"n\">sl</span><span class=\"o\">.</span><span class=\"n\">frb</span><span class=\"p\">[</span><span class=\"s1\">&#39;density&#39;</span><span class=\"p\">]</span> <span class=\"o\">&gt;</span> <span class=\"mf\">0.0</span><span class=\"p\">)</span>\n    <span class=\"k\">except</span><span class=\"p\">:</span>\n        <span class=\"k\">print</span> <span class=\"s1\">&#39;Axis </span><span class=\"si\">%s</span><span class=\"s1\"> failed&#39;</span> <span class=\"o\">%</span> <span class=\"n\">ax</span>\n</pre></div>",
